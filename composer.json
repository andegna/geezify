{
	"name": "andegna/geezify",
	"description": "Simple library to convert ascii number '3456' to geez number '፴፬፻፶፮'.",
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
		{
			"name": "Sam As End",
			"email": "4sam21@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4|7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"Andegna\\Geez\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Andegna\\Geez\\PHPUnit\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"develop": "1.0.x-dev"
		}
	}
}
